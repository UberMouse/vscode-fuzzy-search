{
  "name": "fuzzy-search",
  "displayName": "Fuzzy search",
  "description": "Change the default search for fzf",
  "publisher": "tatosjb",
  "version": "1.4.0",
  "repository": {
    "url": "https://github.com/tatosjb/vscode-fuzzy-search"
  },
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.fuzzySearch"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.fuzzySearch",
        "title": "Fuzzy search"
      }
    ],
    "keybindings": [
      {
        "command": "extension.fuzzySearch",
        "title": "Fuzzy search",
        "key": "ctrl+k ctrl+f",
        "mac": "cmd+k cmd+f"
      },
      {
        "command": "extension.select$1",
        "title": "Select 1st on the list",
        "key": "ctrl+1",
        "when": "extension == tatosjb.fuzzy-search"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./ && cp -R ./binaries ./out",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.0",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "glob": "^7.1.6",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "fast-fuzzy": "^1.10.10"
  }
}
